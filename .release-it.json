{
  "git": {
    "tagName": "v${version}",
    "requireCleanWorkingDir": false,
    "requireUpstream": false,
    "commit": false,
    "pushArgs": "--tags",
    "pushRepo": null
  },
  "npm": false,
  "github": {
    "release": true,
    "assets": null
  },
  "pkgFiles": null,
  "plugins": {
    "@release-it/conventional-changelog": {
      "preset": "angular"
    }
  }
}
