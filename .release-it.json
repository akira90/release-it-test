{
  "git": {
    "tagName": "v${version}",
    "requireCleanWorkingDir": false,
    "requireUpstream": false,
    "commit": false
  },
  "npm": {
    "publish": false
  },
  "github": {
    "release": true,
    "assets": null
  },
  "pkgFiles": null,
  "plugins": {
    "@release-it/conventional-changelog": {
      "preset": "angular",
      "infile": "CHANGELOG.md"
    }
  }
}
